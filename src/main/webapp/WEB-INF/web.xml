<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>IndexServlet</servlet-name>
    <servlet-class>ru.job4j.dream.servlet.IndexServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern>/index.do</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>PostServlet</servlet-name>
    <servlet-class>ru.job4j.dream.servlet.PostServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PostServlet</servlet-name>
    <url-pattern>/post.do</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>PostEditServlet</servlet-name>
    <servlet-class>ru.job4j.dream.servlet.PostEditServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PostEditServlet</servlet-name>
    <url-pattern>/post/save</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PostEditServlet</servlet-name>
    <url-pattern>/post/edit.do</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>CandidateServlet</servlet-name>
    <servlet-class>ru.job4j.dream.servlet.CandidateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CandidateServlet</servlet-name>
    <url-pattern>/candidate.do</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>CandidateEditServlet</servlet-name>
    <servlet-class>ru.job4j.dream.servlet.CandidateEditServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CandidateEditServlet</servlet-name>
    <url-pattern>/candidate/save</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CandidateEditServlet</servlet-name>
    <url-pattern>/candidate/edit.do</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>CandidateDeleteServlet</servlet-name>
    <servlet-class>ru.job4j.dream.servlet.CandidateDeleteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CandidateDeleteServlet</servlet-name>
    <url-pattern>/candidate/delete</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>CandidatePhotoServlet</servlet-name>
    <servlet-class>ru.job4j.dream.servlet.CandidatePhotoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CandidatePhotoServlet</servlet-name>
    <url-pattern>/candidate/photo</url-pattern>
  </servlet-mapping>

</web-app>
